<h1> <%= feed.escape("title") %> </h1>
<button class="refresh">Refresh</button>
<button class="back">Back to Index</button>
<ul>
  <% feed.get("entries").each(function(entry){ %>
    <li>
      <a href="#feeds/<%= feed.escape('id') %>/entries/<%= entry.escape('id') %>">
      <%= entry.escape('title') %></a>
    </li>
  <% }) %>
</ul>